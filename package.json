{
  "name": "app-neo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "mock": "ng serve -c mock",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "unused": "eslint --fix ./src/app",
    "format-quick": "pretty-quick --staged",
    "enabled-husky": "husky install",
    "test": "ng test",
    "coverage": "npx open-cli ./coverage/index.html && ng test --code-coverage",
    "test-report": "ng test --watch=false --code-coverage",
    "build:production": "ng build --configuration=production",
    "build:testing": "ng build --configuration=testing",
    "build:stage": "ng build --configuration=stage",
    "build:analyzer": "ng build --configuration=production --stats-json",
    "analyzer": "npx webpack-bundle-analyzer dist/app-neo/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@ngrx/store": "^16.2.0",
    "@ngrx/store-devtools": "^16.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "crypto-js": "^4.1.1",
    "jwt-decode": "^3.1.2",
    "luxon": "^3.2.1",
    "ngrx-store-localstorage": "^16.0.0",
    "open-cli": "^7.1.0",
    "pretty-quick": "^3.1.3",
    "primeicons": "^6.0.1",
    "primeng": "^16.2.0",
    "rxjs": "^7.8.1",
    "sweetalert2": "^11.7.28",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.1.1",
    "@angular-eslint/eslint-plugin": "^16.1.1",
    "@angular-eslint/eslint-plugin-template": "^16.1.1",
    "@angular-eslint/schematics": "^16.1.1",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "^16.2.1",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jasmine": "~4.3.0",
    "@types/luxon": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "^8.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.1.6",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1"
  }
}
